        <field name="key">l10n_mx_edi_retailer.buyer_gln</field>
        <field name="value">--BuyersGLN--</field>
    </record>

    <record id="param_seller_gln" model="ir.config_parameter">
        <field name="key">l10n_mx_edi_retailer.seller_gln</field>
