<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="us_letter_big" model="report.paperformat">
        <field name="name">US Letter Big</field>
        <field name="default" eval="True" />
        <field name="format">Letter</field>
        <field name="page_height" eval="0" />
        <field name="page_width" eval="0" />
        <field name="orientation">Portrait</field>
        <field name="margin_top" eval="40" />
        <field name="margin_bottom" eval="30" />
        <field name="margin_left" eval="7" />
        <field name="margin_right" eval="7" />
        <field name="header_line" eval="False" />
        <field name="header_spacing" eval="35" />
        <field name="dpi" eval="110" />
    </record>
</odoo>
